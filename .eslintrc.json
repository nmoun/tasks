{
    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true
    },

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-unused-vars": "warn",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    },
    "plugins": [
        "react"
    ]
}